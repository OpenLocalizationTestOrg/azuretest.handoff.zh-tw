          <source>% of total system memory available/PageFileBytes.</source>
          <target state="new">% of total system memory available/PageFileBytes.</target>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Total available PageFileBytes is approximately 2 times the RAM of the system.</source>
          <target state="new">Total available PageFileBytes is approximately 2 times the RAM of the system.</target>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>60%</source>
          <target state="new">60%</target>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>70%</source>
          <target state="new">70%</target>
        </trans-unit>
