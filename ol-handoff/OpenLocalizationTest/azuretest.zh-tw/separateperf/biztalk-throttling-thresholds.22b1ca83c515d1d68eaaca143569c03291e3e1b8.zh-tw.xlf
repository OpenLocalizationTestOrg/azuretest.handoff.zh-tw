        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Learn about Throttling in BizTalk Services | Microsoft Azure</source>
          <target state="new">Learn about Throttling in BizTalk Services | Microsoft Azure</target>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Learn about throttling thresholds and resulting runtime behaviors for BizTalk Services.</source>
          <target state="new">Learn about throttling thresholds and resulting runtime behaviors for BizTalk Services.</target>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Throttling is based on memory usage and number of messages.</source>
          <target state="new">Throttling is based on memory usage and number of messages.</target>
        </trans-unit>
