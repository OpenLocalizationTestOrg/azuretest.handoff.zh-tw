          <source>RoleInstanceA is throttling.</source>
          <target state="new">RoleInstanceA is throttling.</target>
        </trans-unit>
        <trans-unit id="134" translate="yes" xml:space="preserve">
          <source>RoleInstanceB is not throttling.</source>
          <target state="new">RoleInstanceB is not throttling.</target>
        </trans-unit>
        <trans-unit id="135" translate="yes" xml:space="preserve">
          <source>In this situation, messages in RoleInstanceB are processed as expected.</source>
          <target state="new">In this situation, messages in RoleInstanceB are processed as expected.</target>
        </trans-unit>
        <trans-unit id="136" translate="yes" xml:space="preserve">
          <source>Messages in RoleInstanceA are discarded and fail with the following error:</source>
          <target state="new">Messages in RoleInstanceA are discarded and fail with the following error:</target>
        </trans-unit>
        <trans-unit id="137" translate="yes" xml:space="preserve">
          <source>Server is busy.</source>
          <target state="new">Server is busy.</target>
        </trans-unit>
        <trans-unit id="138" translate="yes" xml:space="preserve">
          <source>Please try again.</source>
          <target state="new">Please try again.</target>
        </trans-unit>
        <trans-unit id="140" translate="yes" xml:space="preserve">
          <source>Any pull sources do not poll or download a message.</source>
          <target state="new">Any pull sources do not poll or download a message.</target>
        </trans-unit>
