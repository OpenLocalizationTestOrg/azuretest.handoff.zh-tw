          <source>When Azure BizTalk Services enters a throttling state, the following occurs:</source>
          <target state="new">When Azure BizTalk Services enters a throttling state, the following occurs:</target>
        </trans-unit>
        <trans-unit id="131" translate="yes" xml:space="preserve">
          <source>Throttling is per role instance.</source>
          <target state="new">Throttling is per role instance.</target>
        </trans-unit>
