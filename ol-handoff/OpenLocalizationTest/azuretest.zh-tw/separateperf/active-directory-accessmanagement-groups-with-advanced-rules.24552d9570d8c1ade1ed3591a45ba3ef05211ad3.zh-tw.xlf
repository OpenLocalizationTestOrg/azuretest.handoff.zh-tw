          <source>The following are examples of a properly constructed advanced rule:</source>
          <target state="new">The following are examples of a properly constructed advanced rule:</target>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>(user.department -eq "Sales") -or (user.department -eq "Marketing")</source>
          <target state="new">(user.department -eq "Sales") -or (user.department -eq "Marketing")</target>
        </trans-unit>
