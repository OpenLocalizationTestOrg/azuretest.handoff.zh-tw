    {
      "pos": [
        5254,
        5304
      ],
      "content": "<ph id=\"ph1\">&lt;a id=\"stgtools\"&gt;</ph><ph id=\"ph2\">&lt;/a&gt;</ph>Microsoft Azure Storage Tools"
    },
