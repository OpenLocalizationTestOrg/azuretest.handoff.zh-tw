  <file datatype="xml" original="md" source-language="en-US" target-language="zh-tw">
    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>You can subscribe to an Azure-hosted, fully-managed MongoDB database in the Azure Store.</source>
          <target state="new">You can subscribe to an Azure-hosted, fully-managed MongoDB database in the Azure Store.</target>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>To do so, follow these steps:</source>
          <target state="new">To do so, follow these steps:</target>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Log into the Azure Management Portal.</source>
          <target state="new">Log into the Azure Management Portal.</target>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>New<ept id="p1">**</ept>.</source>
          <target state="new">Click <bpt id="p1">**</bpt>New<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source><ph id="ph1">![</ph>New<ph id="ph2">][button-new]</ph></source>
          <target state="new"><ph id="ph1">![</ph>New<ph id="ph2">][button-new]</ph></target>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Select <bpt id="p1">**</bpt>Store<ept id="p1">**</ept>.</source>
          <target state="new">Select <bpt id="p1">**</bpt>Store<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source><ph id="ph1">![</ph>Store<ph id="ph2">][button-store]</ph></source>
          <target state="new"><ph id="ph1">![</ph>Store<ph id="ph2">][button-store]</ph></target>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Select MongoLab.</source>
          <target state="new">Select MongoLab.</target>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>You can find us in the <bpt id="p1">**</bpt>App Services<ept id="p1">**</ept> category, as well as under <bpt id="p2">**</bpt>All<ept id="p2">**</ept>.</source>
          <target state="new">You can find us in the <bpt id="p1">**</bpt>App Services<ept id="p1">**</ept> category, as well as under <bpt id="p2">**</bpt>All<ept id="p2">**</ept>.</target>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source><ph id="ph1">![</ph>MongoLab<ph id="ph2">][entry-mongolab]</ph></source>
          <target state="new"><ph id="ph1">![</ph>MongoLab<ph id="ph2">][entry-mongolab]</ph></target>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>Next<ept id="p1">**</ept>.</source>
          <target state="new">Click <bpt id="p1">**</bpt>Next<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source><ph id="ph1">![</ph>Next<ph id="ph2">][button-next]</ph></source>
          <target state="new"><ph id="ph1">![</ph>Next<ph id="ph2">][button-next]</ph></target>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>The MongoLab store entry displays.</source>
          <target state="new">The MongoLab store entry displays.</target>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source><ph id="ph1">![</ph>NewMongoLab<ph id="ph2">][screen-newmongolab]</ph></source>
          <target state="new"><ph id="ph1">![</ph>NewMongoLab<ph id="ph2">][screen-newmongolab]</ph></target>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Select your desired <bpt id="p1">**</bpt>Subscription<ept id="p1">**</ept> option.</source>
          <target state="new">Select your desired <bpt id="p1">**</bpt>Subscription<ept id="p1">**</ept> option.</target>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>Enter a <bpt id="p1">**</bpt>Name<ept id="p1">**</ept> for your database.</source>
          <target state="new">Enter a <bpt id="p1">**</bpt>Name<ept id="p1">**</ept> for your database.</target>
        </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve">
          <source>Your name can only contain alphanumeric characters, dashes, dots, and underscores.</source>
          <target state="new">Your name can only contain alphanumeric characters, dashes, dots, and underscores.</target>
        </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve">
          <source>MongoLab also requires that this name be unique, so you may be asked to re-submit your request if the name is taken.</source>
          <target state="new">MongoLab also requires that this name be unique, so you may be asked to re-submit your request if the name is taken.</target>
        </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>Select your desired <bpt id="p1">**</bpt>Region<ept id="p1">**</ept>.</source>
          <target state="new">Select your desired <bpt id="p1">**</bpt>Region<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>Next<ept id="p1">**</ept>.</source>
          <target state="new">Click <bpt id="p1">**</bpt>Next<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve">
          <source><ph id="ph1">![</ph>Next<ph id="ph2">][button-next]</ph></source>
          <target state="new"><ph id="ph1">![</ph>Next<ph id="ph2">][button-next]</ph></target>
        </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve">
          <source>Review your store purchase information, and then click <bpt id="p1">**</bpt>Purchase<ept id="p1">**</ept> to confirm.</source>
          <target state="new">Review your store purchase information, and then click <bpt id="p1">**</bpt>Purchase<ept id="p1">**</ept> to confirm.</target>
        </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source><ph id="ph1">![</ph>Next<ph id="ph2">][button-purchase]</ph></source>
          <target state="new"><ph id="ph1">![</ph>Next<ph id="ph2">][button-purchase]</ph></target>
        </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve">
          <source>The toolbar progress button provides your provisioning status.</source>
          <target state="new">The toolbar progress button provides your provisioning status.</target>
        </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve">
          <source>ProgressButton</source>
          <target state="new">ProgressButton</target>
        </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve">
          <source>A success message displays when provisioning completes.</source>
          <target state="new">A success message displays when provisioning completes.</target>
        </trans-unit>
        <trans-unit id="127" translate="yes" xml:space="preserve">
          <source>SuccessMessage</source>
          <target state="new">SuccessMessage</target>
        </trans-unit>
        <trans-unit id="128" translate="yes" xml:space="preserve">
          <source>Congratulations!</source>
          <target state="new">Congratulations!</target>
        </trans-unit>
        <trans-unit id="129" translate="yes" xml:space="preserve">
          <source>MongoLab has just provisioned a MongoDB database in the Azure region you selected.</source>
          <target state="new">MongoLab has just provisioned a MongoDB database in the Azure region you selected.</target>
        </trans-unit>
        <trans-unit id="130" translate="yes" xml:space="preserve">
          <source>You now have access to our management UI and 24/7 support.</source>
          <target state="new">You now have access to our management UI and 24/7 support.</target>
        </trans-unit>
      </group>
