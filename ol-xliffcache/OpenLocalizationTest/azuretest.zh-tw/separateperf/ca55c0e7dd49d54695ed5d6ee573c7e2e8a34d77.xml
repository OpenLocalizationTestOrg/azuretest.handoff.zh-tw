    {
      "content": "Create Storage Account",
      "pos": [
        2314,
        2336
      ]
    },
    {
      "content": "In the <bpt id=\"p1\">**</bpt>URL<ept id=\"p1\">**</ept> field, type a subdomain name.",
      "pos": [
        2370,
        2414
      ]
    },
